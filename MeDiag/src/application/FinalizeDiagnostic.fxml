      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="880.0" style="-fx-background-color: #050a30;">
         <children>
            <HBox layoutX="26.0" layoutY="128.0" prefHeight="387.0" prefWidth="842.0" spacing="30.0">
               <children>
                  <VBox fx:id="questionPane" prefHeight="386.0" prefWidth="858.0" spacing="30.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" layoutY="-3.0" prefHeight="58.0" prefWidth="880.0">
               <children>
                  <Label alignment="CENTER" prefHeight="58.0" prefWidth="437.0" text="Diagnostic Questions" textFill="#a4d1e7">
                     <font>
                        <Font size="32.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <Button fx:id="homeButton" graphicTextGap="0.0" layoutX="775.0" layoutY="535.0" mnemonicParsing="false" onAction="#loadHomePage" prefHeight="42.0" prefWidth="42.0" styleClass="transparent" stylesheets="@application.css" textAlignment="CENTER">
